{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": "error",
    "require-jsdoc": "off",
    "max-len": ["error", 120],
    "comma-dangle": ["error", "never"],
    "max-depth": ["error", 3],
    "no-unused-vars": ["error", { "args": "all" }]
  }
}